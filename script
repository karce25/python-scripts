import csv

input_csv = "vlans.csv"
output_txt = "f5os_vlan_commands.txt"

with open(input_csv, newline='') as csvfile, open(output_txt, "w") as outfile:
    reader = csv.DictReader(csvfile)
    for row in reader:
        vlan_name = row["vlan_name"].strip()
        vlan_id = row["id"].strip()
        # Skip row if any value is missing
        if not vlan_name or not vlan_id:
            continue
        command = f"vlans vlan {vlan_id} config name {vlan_name} vlan-id {vlan_id}"
        outfile.write(command + "\n")

print(f"Commands written to {output_txt}")
